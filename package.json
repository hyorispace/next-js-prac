{
  "name": "next-js-prac",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "dev": "next dev -p 3010",
    "build": "next build",
    "lint": "next lint",
    "server": "node server.js",
    "toc": "i18next-resources-for-ts toc -i ./public/locales/en -o ./src/types/resources.ts",
    "merge": "i18next-resources-for-ts merge -i ./public/locales/en -o ./src/types/resources.json",
